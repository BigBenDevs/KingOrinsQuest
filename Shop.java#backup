    /**
     * Controls the spawning of player when transfering between worlds
     */
    public void spawnPlayer()
    {
        if (MainWorld.spawn == 1)
        {
            Player hawthorne = new Player();
            addObject(hawthorne, 285,350);
        }
    }
    
    
